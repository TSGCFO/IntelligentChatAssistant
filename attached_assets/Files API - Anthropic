.latex-style pre { font-size: 85%; padding: 1rem 1.4rem; max-width: 100%; overflow: auto; border-radius: 4px; background: rgb(232, 237, 242); }

.latex-style pre code { position: relative; }

.latex-style kbd { background: rgb(255, 255, 255); border: 1px solid rgb(175, 179, 182); border-radius: 2px; padding: 2px 4px; font-size: 75%; }

.latex-style table { border-collapse: collapse; border-spacing: 0px; width: auto; max-width: 100%; border-top: 2.27px solid rgb(0, 0, 0); border-bottom: 2.27px solid rgb(0, 0, 0); counter-increment: caption 1; }

.latex-style table tr > th[scope="col"] { border-bottom: 1.36px solid rgb(0, 0, 0); }

.latex-style table tr > th[scope="row"] { border-right: 1.36px solid rgb(0, 0, 0); }

.latex-style table > tbody > tr:first-child > td { border-top: 1.36px solid rgb(0, 0, 0); }

.latex-style table > tbody > tr:first-child > th, .latex-style table > tbody > tr:last-child > td, .latex-style table > tbody > tr:last-child > th { border-bottom: 1.36px solid rgb(0, 0, 0); }

.latex-style td, .latex-style th { text-align: left; padding: 0.5rem; line-height: 1.1; }

.latex-style caption { text-align: left; font-size: 0.923em; padding: 0px 0.25em 0.25em; width: 100%; margin-left: 0px; }

.latex-style caption::before { content: "Table " counter(caption) ". "; font-weight: 700; }

.latex-style h1 { font-size: 2.5rem; line-height: 3.25rem; margin-bottom: 1.625rem; }

.latex-style h1:first-child { text-align: center; }

.latex-style h1 { font-weight: 800; }

.latex-style h2 { font-size: 1.7rem; line-height: 2rem; margin-top: 3rem; margin-bottom: 0.8rem; font-weight: 700; }

.latex-style h3 { font-size: 1.4rem; margin-top: 2.5rem; }

.latex-style h3, .latex-style h4 { margin-bottom: 0.8rem; line-height: 1.625rem; font-weight: 600; }

.latex-style h4 { font-size: 1.2rem; margin-top: 2rem; }

.latex-style h5 { margin-top: 1.8rem; font-weight: 600; }

